<div class="messages">
  <div class="container">

    <!-- Your code here -->
    <% @posts.each do |post| %>
        <div class="message">
          <p class="content"><%= post.content %></p>
          <p class="time"><%= post.created_at %></p>
        </div>
    <% end %>
    <%= link_to 'New Post', "posts/new" %>

  </div>
</div>